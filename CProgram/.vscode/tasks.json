{
    // See https://go.microsoft.com/fwlink/?LinkId=733558
    // for the documentation about the tasks.json format
    "version": "2.0.0",
    "tasks": [
        {
            "label": "build",
            "type": "shell",
            "command": "gcc",
            "args": [
                "./mihInclude/mihdatastruct.h",
                "./graph.c",
                "./linktable.c",
                "./queue.c",
                "-o",
                "E:/Cexe/graph.exe",
                "-g"
            ],
            "problemMatcher": [
                "$gcc"
            ]
        },
        {
            "label": "mihTest",
            "type": "shell",
            "command": "gcc",
            "args": [
                
                "-o",
                "E:/",
                "-g"
            ],
            "problemMatcher": [
                "$gcc"
            ]
        }
    ]
}