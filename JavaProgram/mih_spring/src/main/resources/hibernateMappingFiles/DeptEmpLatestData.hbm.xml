<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="model.DeptEmpLatestData" table="dept_emp_latest_date">
        <composite-id>
            <key-property name="empNo" column="emp_no" type="java.lang.Integer" length="11" />
            <key-property name="fromDate" column="from_date" type="java.util.Date" />
            <key-property name="toDate" column="to_date" type="java.util.Date" />
        </composite-id>
    </class>
</hibernate-mapping>