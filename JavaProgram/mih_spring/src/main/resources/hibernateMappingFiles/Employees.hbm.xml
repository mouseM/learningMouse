<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="model.Employees" table="employees">
        <id name="empNo" column="emp_no" type="java.lang.Integer" />
        <property name="birthDate" column="birth_date" type="java.util.Date" />
        <property name="firstName" column="first_name" type="java.lang.String" />
        <property name="lastName" column="last_name" type="java.lang.String" />
        <property name="hireDate" column="hire_date" type="java.util.Date" />
    </class>
</hibernate-mapping>