<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="model.Salaries" table="salaries">
        <composite-id>
            <key-property name="empNo" column="emp_no" type="java.lang.Integer" />
            <key-property name="fromDate" column="from_date" type="java.util.Date" />
        </composite-id>
        <property name="salary" column="salary" type="java.lang.Integer" />
        <property name="toDate" column="to_date" type="java.util.Date" />
    </class>
</hibernate-mapping>